    additionalScripts: `
        // Your inline JavaScript code here
        // Get elements by ID
        var jsopacity = document.getElementById('js_opacity');
        var jsopacityText = document.getElementById('js_opacity_class');
        var opacityRange = document.getElementById('js_opacity_range');
        
        // Add event listeners for both input and change events
        ['input', 'change'].forEach(function(event) {
            opacityRange.addEventListener(event, function() {
                var value = this.value;
                jsopacityText.textContent = '.opacity' + value;
                
                // Remove all classes and add new ones
                jsopacity.className = '';
                jsopacity.className = 'rounded-circle width-3 h-auto opacity-' + value;
            });
        });
        
        // Gradient example
        var ng_bgColors;
        var ng_bgColors_URL = './json/ng-bg-colors.json';
        var formatBgColors = [];
        
        // Create XMLHttpRequest for JSON data
        var xhr = new XMLHttpRequest();
        xhr.open('GET', ng_bgColors_URL, true);
        
        xhr.onload = function() {
            if (xhr.status === 200) {
                ng_bgColors = JSON.parse(xhr.responseText);
                
                if (ng_bgColors) {
                    // Create default option
                    var defaultOption = document.createElement('option');
                    defaultOption.value = '';
                    defaultOption.textContent = 'select background';
                    formatBgColors.push(defaultOption);
                    
                    // Create options for each background color
                    ng_bgColors.forEach(function(item) {
                        var option = document.createElement('option');
                        option.value = item;
                        option.className = item;
                        option.textContent = item;
                        formatBgColors.push(option);
                    });
                    
                    // Get all select elements with class js-bg-color and update them
                    var selectElements = document.getElementsByClassName('js-bg-color');
                    Array.prototype.forEach.call(selectElements, function(select) {
                        // Clear existing options
                        select.innerHTML = '';
                        // Append new options
                        formatBgColors.forEach(function(option) {
                            select.appendChild(option.cloneNode(true));
                        });
                    });
                } else {
                    console.log('something went wrong!');
                }
            }
        };
        
        xhr.onerror = function() {
            console.log('something went wrong!');
        };
        
        xhr.send();
        
        // Helper function to remove classes with prefix
        function removeClassByPrefix(element, prefix) {
            var classes = element.className.split(' ');
            classes = classes.filter(function(c) {
                return c.lastIndexOf(prefix, 0) !== 0;
            });
            element.className = classes.join(' ');
        }
        
        // Change background handler
        document.addEventListener('change', function(e) {
            if (e.target.classList.contains('js-bg-gradient-demo')) {
                var colorSelect = document.querySelector('select.js-bg-color');
                var gradientSelect = document.querySelector('select.js-bg-gradient');
                var gradientDemo = document.getElementById('js-gradient-demo');
                
                var setBgColor = colorSelect.value;
                var setValue = gradientSelect.value;
                
                // Remove bg- prefixed classes and add new class
                removeClassByPrefix(colorSelect, 'bg-');
                colorSelect.classList.add(setBgColor);
                
                // Update gradient demo classes
                gradientDemo.className = 'p-3 rounded ' + setBgColor + ' ' + setValue;
            }
        });
    `,
